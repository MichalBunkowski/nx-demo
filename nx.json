{
  "npmScope": "nx-demo",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "next-app": {
      "tags": []
    },
    "next-app-e2e": {
      "tags": [],
      "implicitDependencies": ["next-app"]
    },
    "shared-ui": {
      "tags": []
    },
    "another-app": {
      "tags": []
    },
    "another-app-e2e": {
      "tags": [],
      "implicitDependencies": ["another-app"]
    }
  }
}
